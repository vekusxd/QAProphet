{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq:5341"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "AllowedHosts": "*",
  "QuestionTimeout": {
    "EditQuestionInMinutes": 60,
    "DeleteQuestionInMinutes": 60,
    "EditCommentInMinutes": 60,
    "DeleteCommentInMinutes": 60
  },
  "AnswerTimeout": {
    "EditAnswerInMinutes": 60,
    "DeleteAnswerInMinutes": 60,
    "EditCommentInMinutes": 60,
    "DeleteCommentInMinutes": 60
  }
}
